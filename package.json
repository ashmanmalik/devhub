{
  "name": "dev-portal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --dotenv .env.local",
    "build": "nuxt build",
    "start": "NODE_ENV=staging nuxt --hostname 0.0.0.0 --dotenv .env.local",
    "pages-to-algolia": "node scripts/algolia.js",
    "generate:mgt": "node scripts/mgt.js",
    "generate:schemas": "node scripts/schema.js",
    "build:staging": "npm run generate:mgt -- --dotenv .env.staging && npm run generate:schemas -- --dotenv .env.staging && nuxt generate --dotenv .env.staging",
    "build:production": "npm run generate:mgt -- --dotenv .env.production && npm run generate:schemas -- --dotenv .env.production && nuxt generate --dotenv .env.production && npm run pages-to-algolia -- --dotenv .env.production",
    "lint:js": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "deploy": "pm2 deploy production update",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@chargetrip/internal-vue-components": "0.0.576",
    "@nuxt/content": "^1.14.0",
    "@nuxt/image": "^0.5.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/pwa": "^3.3.2",
    "algoliasearch": "^4.8.1",
    "cloudinary": "^1.26.3",
    "cookie": "^0.4.1",
    "core-js": "^3.7.0",
    "cors": "^2.8.5",
    "cross-fetch": "^3.0.6",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "fsevents": "^2.3.1",
    "graphql": "^15.3.0",
    "highlight.js": "^10.4.0",
    "js-convert-case": "^4.1.0",
    "js-cookie": "^2.2.1",
    "jsdom": "^16.4.0",
    "marked": "^2.0.3",
    "nuxt": "^2.15.3",
    "path": "^0.12.7",
    "showdown": "^1.9.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.5",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^4.0.2",
    "@types/js-cookie": "^2.2.6",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fibers": "^5.0.0",
    "flags": "^0.1.3",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.5.2",
    "nuxt-property-decorator": "^2.8.8",
    "pm2": "^4.5.6",
    "postcss": "^8.2.8",
    "prettier": "^2.2.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.4.4",
    "vue-jest": "^3.0.4",
    "vue-property-decorator": "^9.1.2",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.1"
  }
}
